{
  "name": "angular2-webpack-electron",
  "version": "0.0.1",
  "description": "Angular2, Webpack and Electron seed project",
  "main": "main.js",
  "scripts": {
    "pkg.darwin.x64": "electron-packager . --platform=darwin --arch=x64 --out=releases/ --overwrite --asar",
    "pkg.linux.x64": "electron-packager . --platform=linux --arch=x64 --out=releases/ --overwrite --asar",
    "pkg.linux.ia32": "electron-packager . --platform=linux --arch=ia32 --out=releases/ --overwrite --asar",
    "pkg.win32.x64": "electron-packager . --platform=win32 --arch=x64 --out=releases/ --overwrite --asar",
    "pkg.win32.ia32": "electron-packager . --platform=win32 --arch=ia32 --out=releases/ --overwrite --asar",
    "pkg.all": "electron-packager . --all --out=releases/ --overwrite --asar",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build.pkg": "npm run build && npm run pkg.all",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "electron": "electron .",
    "start": "npm run build && npm run electron"
  },
  "author": "infining",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.2.5",
    "electron-reload": "^0.3.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular2-material/button": "^2.0.0-alpha.5-2",
    "@angular2-material/card": "^2.0.0-alpha.5-2",
    "@angular2-material/checkbox": "^2.0.0-alpha.5-2",
    "@angular2-material/core": "^2.0.0-alpha.5-2",
    "@angular2-material/grid-list": "^2.0.0-alpha.5-2",
    "@angular2-material/icon": "^2.0.0-alpha.5-2",
    "@angular2-material/input": "^2.0.0-alpha.5-2",
    "@angular2-material/list": "^2.0.0-alpha.5-2",
    "@angular2-material/progress-bar": "^2.0.0-alpha.5-2",
    "@angular2-material/progress-circle": "^2.0.0-alpha.5-2",
    "@angular2-material/radio": "^2.0.0-alpha.5-2",
    "@angular2-material/sidenav": "^2.0.0-alpha.5-2",
    "@angular2-material/slide-toggle": "^2.0.0-alpha.5-2",
    "@angular2-material/tabs": "^2.0.0-alpha.5-2",
    "@angular2-material/toolbar": "^2.0.0-alpha.5-2",
    "es6-shim": "^0.35.1",
    "file-loader": "^0.9.0",
    "material-design-lite": "^1.1.3",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Infining/angular2-webpack-electron.git"
  }
}
